	<queryString>
		<![CDATA[SELECT *, SUM(PP.PP_precio * PP.PP_producto_cantidad) AS TOTAL_PEDIDO FROM Producto PR, Producto_pedidos PP ,Pedido P, tipo_producto T, Cliente C
WHERE
T.TP_id= PR.PR_tipo_producto AND
PR.Pr_id=PP.PP_productoid AND
P.PD_id=PP.PP_pedidoid AND
P.PD_cliente=C.CL_id AND
P.PD_ID = $P{ID_PEDIDO};]]>
	</queryString>
