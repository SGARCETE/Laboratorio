	<queryString language="SQL">
		<![CDATA[select P.PD_id, PR.Pr_nombre, P.PD_fecha_pedido, C.CL_nombre,C.CL_telefono,(PP.PP_precio*PP.PP_producto_cantidad) as total, PR.Pr_observacion, C.CL_direccion, T.TP_nombre,  PP.PP_producto_cantidad,
